<?xml version="1.0" encoding="UTF-8"?>
<database>

    <global>
        <table name="translate">
            <field type="INT(11) NOT NULL PRIMARY KEY">id</field>
            <field type="varchar(255)">groups</field>
            <field type="varchar(255)">var</field>
            <field type="varchar(20) DEFAULT ''">datatype</field>
            <field type="varchar(200) DEFAULT ''">datadefine</field>
            <field type="int(1) DEFAULT 0">html</field>
            <field type="int(4) DEFAULT 0">priority</field>
            <field type="varchar(255) DEFAULT NULL">package</field>

            <primary>id</primary>
            <auto_increment>id</auto_increment>
            <index>package</index>
            <index>groups</index>
            <index>var</index>
        </table>
    </global>

</database>
